<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_private_policy"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.shoutin.login.PrivacyPolicyActivity">


    <Button
        android:id="@+id/private_policies_back"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/red_background_color"
        android:text="BACK"
        android:textColor="@color/white_text_color" />

    <WebView
        android:id="@+id/webview_private_policy"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/private_policies_back">

    </WebView>

</RelativeLayout>
